  "main": "index.js",
  "scripts": {
    "server": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node build",
    "db-init": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
