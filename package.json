    "pg-hstore": "^2.3.4",
    "randomstring": "^1.3.0",
    "sequelize": "^6.33.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
